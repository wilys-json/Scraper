{
    "config": {
        "BaseUrl": "https://www.facebook.com",
        "TargetUrl": "https://www.facebook.com/$ALIAS/photos",
        "Wait": 0.5,
        "LongWait": 1.5,
        "downloadDirectory": "PhotosOfMe",
        "downloadCounts": 0,
        "downloadFileFormat": ".jpg",
        "downloadBuffer": "photoUrl",
        "usernameElement": {
            "type": "id",
            "keyword": "email",
            "content": "$EMAIL",
            "encoded": false,
            "action": "_fillContent"
        },
        "passwordElement": {
            "type": "id",
            "keyword": "pass",
            "content": "$ENCODED_PASSWORD",
            "encoded": true,
            "reverse": true,
            "action": "_fillContent"
        },
        "loginSubmitButton": {
            "type": "id",
            "keyword": [
                "loginbutton",
                "u_0_b"
            ],
            "action": "_tryClicks"
        },
        "approvalCode": {
            "type": "id",
            "keyword": "approvals_code",
            "encoded": false,
            "action": "_fillContent"
        },
        "checkpointSubmitButton": {
            "type": "id",
            "keyword": "checkpointSubmitButton",
            "action": "_click"
        },
        "saveBrowser": {
            "type": "xpath",
            "keyword": "//span[.='Save Browser']",
            "action": "_click"
        },
        "recursiveScroll": {
            "action": "_scroll",
            "recursive": true
        },
        "getPhotoUrls": {
            "type": "xpath",
            "keyword": "//a[contains(@href, \"php?fbid\")]",
            "action": "_findObjects",
            "inventory": "photoUrls",
            "attribute": "href"
        },
        "scrapePhotos": {
            "action": "_scrapeFromUrl",
            "scrapeActions": [
                "locatePhoto",
                "getPhoto"
            ],
            "inventory": "photoUrls"
        },
        "locatePhoto": {
            "type": "xpath",
            "keyword": "//img[contains(@src, \"jpg\")]",
            "action": "_findObject",
            "inventory": "photoUrl",
            "attribute": "src"
        },
        "getPhoto": {
            "action": "_downloadObject"
        },
        "loginSteps": [
            "usernameElement",
            "passwordElement",
            "loginSubmitButton",
            "approvalCode",
            "checkpointSubmitButton",
            "saveBrowser",
            "checkpointSubmitButton"
        ],
        "scrapeSteps": [
            "recursiveScroll",
            "getPhotoUrls",
            "scrapePhotos"
        ]
    },
    "steps": [
        "gotoBaseUrl",
        "login",
        "gotoTargetUrl",
        "scrape"
    ]
}
